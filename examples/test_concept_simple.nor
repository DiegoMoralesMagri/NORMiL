print("Nouveau count: " + to_string(c2.doc_count))
print("Nouveau trust: " + to_string(c2.trust_score))
print("")

// Test 4: Liste de concepts
concepts = [
    Concept {
        concept_id: "c_alpha",
        centroid_vec: vec(2, [1.0, 0.0]),
        doc_count: 10,
        provenance_versions: ["v1"],
        trust_score: 0.8,
        labels: ["cat_a"]
    },
    Concept {
        concept_id: "c_beta",
        centroid_vec: vec(2, [0.0, 1.0]),
        doc_count: 20,
        provenance_versions: ["v1"],
        trust_score: 0.9,
        labels: ["cat_b"]
    }
]
